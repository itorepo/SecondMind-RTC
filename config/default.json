{
	"app": {
		"title": "sm-rtvsp"
	},
	"rtvsp": {
		"server": {
			"host": "localhost",
			"port": 5300,
			"exclusive": false
		},
		"client": {
			"host": "localhost",
			"port": 5300
		},
		"gateway": {
		  	"maxPoolSize": 6,
		  	"host": "localhost",
		  	"port": 5300
		}
	},
	"logging": {
		"error": {
			"transports": [{
				"File": {
					"filename": "sm-rtvsp.err.log"
				}
			}]
		},
		"info": {
			"transports": [{
				"File": {
					"filename": "sm-rtvsp.out.log"
				}
			}]
		}
	}
}